<p id="notice"><%= notice %></p>

<%= link_to 'Nova Emenda', new_emmend_path %>

<ul>
  <% @emmends.each do |emmend| %>
  <li> <%= "#{emmend.name} - Criado por  #{emmend.user.name}" %>
    <%= link_to 'Show', emmend %>
      <% if current_user == emmend.user %>
      <%= link_to 'Edit', edit_emmend_path(emmend) %>
      <%= link_to 'Destroy', emmend, method: :delete, data: { confirm: 'Are you sure?' } %>
      <% end %>
    <% end %>
  </li>
</ul>

